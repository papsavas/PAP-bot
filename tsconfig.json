{
  "compilerOptions": {
    "outDir": "./build",
    "experimentalDecorators": true,
    "target": "ES2019",
    "module": "commonjs",
    "strict": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "baseUrl": "./",
    /*
    "paths": {
      "@root/*" : ["./*"],
      "@cmdInterfaces/*" : ["./Commands/Interf/*"],
      "@cmdImplementations/*" : ["./Commands/Impl/*"],
      "@Commands/*" : ["./Commands/*"],
      "@Inversify/*" : ["./Inversify/*"]
    }*/
  }
}