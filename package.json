{
  "name": "papbotts",
  "version": "2.0.0",
  "description": "PAPbot",
  "main": "src/index.ts",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "launch": "npm run timezone && ts-node src/index.ts",
    "timezone": "set TZ='Europe/Athens'",
    "dev": "set NODE_ENV='development' && npm run launch",
    "prod": "set NODE_ENV='production1' && npm run launch",
    "production": "set NODE_ENV='production' && npm run launch",
    "test": "jest --passWithNoTests"
  },
  "author": "Papageorgiadis Savvas",
  "license": "ISC",
  "dependencies": {
    "discord-api-types": "^0.18.1",
    "discord.js": "^13.3.1",
    "find-config": "^1.0.0",
    "firebase": "^8.3.3",
    "firebase-admin": "^9.6.0",
    "fs": "0.0.1-security",
    "googleapis": "^73.0.0",
    "greek-utils": "^1.3.0",
    "knex": "^0.95.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "node": "^16.6.1",
    "npm": "^7.15.1",
    "pg": "^8.5.1",
    "profanity-js": "^0.1.2",
    "readline": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rss-feed-emitter": "^3.2.2",
    "ts-node": "^9.1.1",
    "url-regex": "^5.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/greek-utils": "^1.2.0",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.168",
    "@types/pg": "^7.14.11",
    "@types/uuid": "^8.3.1",
    "dotenv": "^10.0.0",
    "jest": "^27.0.6",
    "typescript": "^4.4.4"
  }
}